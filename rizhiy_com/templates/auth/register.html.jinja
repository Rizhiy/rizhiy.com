{% extends "auth/base.html.jinja" %}
{% block title %}Register{% endblock %}
{% block auth %}
    <form method="post">
        <input name="username" id="username" required placeholder="username" />
        <input type="password"
               name="password"
               id="password"
               required
               placeholder="password" />
        <input type="submit" value="Register" />
    </form>
{% endblock %}
